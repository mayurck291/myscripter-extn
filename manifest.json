{
  "name": "My Scripter",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Smarter Way To Handle Websites:Custome scripts on custom URLs",
  "permissions": [
    "tabs", "http://*/*", "https://*/*", "unlimitedStorage"
  ],
  "browser_action": {
    "default_icon": "logo.png"
  },
  "background": {
    "scripts": ["js/jquery.js","js/background.js","js/jstorage.js"]
  },
  "options_page": "html/options.html"

}

